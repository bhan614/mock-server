{
  "name": "wallet-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./public/dist ",
    "dll": "webpack --progress --config webpack.config.dll.babel.js",
    "generateEnv": "node setEnv.js",
    "prebuild": "npm install && npm run clean && npm run generateEnv",
    "build": "webpack --progress --colors --config webpack.build.babel.js",
    "postbuild": "sh ./build.sh",
    "buildBeta": "cross-env NODE_ENV=beta npm run build",
    "buildDev": "npm run clean && cross-env NODE_ENV=development npm run generateEnv && webpack --config webpack.build.babel.js --profile --colors --progress --display-reasons --display-error-details",
    "prestart": "npm run clean && cross-env NODE_ENV=development npm run generateEnv",
    "start": "nodemon bin/www"
  },
  "dependencies": {
    "antd": "^2.5.2",
